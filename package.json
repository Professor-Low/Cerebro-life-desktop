{
  "name": "cerebro-desktop",
  "version": "1.0.0",
  "description": "Cerebro - Your AI, Everywhere",
  "author": "Professor (Michael Anthony Lopez)",
  "license": "AGPL-3.0",
  "main": "electron/main.js",
  "scripts": {
    "start": "electron .",
    "start:dev": "cross-env CEREBRO_DEV=1 electron .",
    "start:remote": "cross-env CEREBRO_REMOTE_URL=http://192.168.0.70:3000 electron .",
    "build": "electron-builder",
    "build:win": "electron-builder --win",
    "build:linux": "electron-builder --linux",
    "build:all": "electron-builder --win --linux",
    "build:win-remote": "cross-env CEREBRO_REMOTE_URL=http://192.168.0.70:3000 electron-builder --win -c build/electron-builder-remote.yml",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "electron-store": "^8.1.0"
  },
  "devDependencies": {
    "electron": "^34.0.0",
    "electron-builder": "^25.1.8",
    "cross-env": "^7.0.3"
  },
  "build": {
    "extends": "build/electron-builder.yml"
  }
}
